{
  "name": "hashtag-thu-web",
  "private": true,
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --transformIgnorePatterns \"node_modules/(?!(@material-ui/core|@material-ui/icons|@babel/runtime)/)\"",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "eject": "react-app-rewired eject",
    "lint": "eslint --ext .tsx,.ts,.jsx,.js src/",
    "typecheck": "tsc --noUnusedLocals"
  },
  "dependencies": {
    "@material-ui/core": "4.7.1",
    "@material-ui/icons": "4.5.1",
    "@reach/router": "1.2.1",
    "@types/jest": "24.0.23",
    "@types/node": "12.12.14",
    "@types/reach__router": "1.2.6",
    "@types/react": "16.9.15",
    "@types/react-dom": "16.9.4",
    "babel-plugin-import": "1.13.0",
    "customize-cra": "0.9.1",
    "husky": "3.1.0",
    "lint-staged": "9.5.0",
    "normalize.css": "8.0.1",
    "prettier": "1.19.1",
    "react": "16.12.0",
    "react-app-rewired": "2.1.5",
    "react-dom": "16.12.0",
    "react-helmet": "5.2.1",
    "react-scripts": "3.3.0",
    "source-map-explorer": "2.1.2",
    "typescript": "3.7.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn lint && yarn typecheck"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
